<Input_Parameters>
  <Auto_mode>False</Auto_mode>
  <Features>
     <red_shift_slices>1</red_shift_slices>
     <Surface_Brightness_Samples>
        <Sample_Number>1</Sample_Number>
     </Surface_Brightness_Samples>     
     <Angular_Area_of_Sky>
        <Squar_Degree>10</Squar_Degree>
     </Angular_Area_of_Sky>     
  </Features>
  <Intrinsic_T-L_correlation>
     <rTL_correlation>0.0</rTL_correlation>
  </Intrinsic_T-L_correlation>
  <Limits>
    <F_limit>2.5e-14</F_limit>
    <Mass_limit>5.0e12</Mass_limit>
  </Limits>
  <Models>
     <Lxm_model>Rozo_et_al_2012</Lxm_model>
     <Txm_model>Henry_et_al_2009</Txm_model>
     <Surface_Brightness>beta</Surface_Brightness>
        <SB_beta>0.5</SB_beta>
  </Models>
  <Input_File>
    <File_name>Sample1.fit</File_name>
  </Input_File>
</Input_Parameters>
